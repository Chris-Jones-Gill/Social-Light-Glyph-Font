======================================================================
Gplus               		!                   content: '!';
Facebook            		&#x22;              content: '\22';
Twitter             		#                   content: '#';
Twitter_2           		$                   content: '$';
Youtube             		%                   content: '%';
Vimeo               		&                   content: '&';
Flickr              		&#x27;              content: '\27';
Picasa              		(                   content: '(';
Dribble             		)                   content: ')';
Forrst              		*                   content: '*';
Deviantart          		+                   content: '+';
Github              		,                   content: ',';
Github_2            		-                   content: '-';
Wordpress           		.                   content: '.';
Blogger             		/                   content: '/';
Tumblr              		0                   content: '0';
Yahoo               		1                   content: '1';
